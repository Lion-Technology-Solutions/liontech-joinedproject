pipeline{
	agent  any
	tools{
	  maven "maven3.9.9"
	}

	stages{
	  stage('clonefromscm'){
	    steps{
	    sh "echo jekins to source code from scm"
	    git branch: 'dec13', url: 'https://github.com/tenbanliontech/liontech-joinedproject.git'
	    }
	  }

	  stage('maven-build'){
	    steps{
	    sh "mvn install"
	    sh "mvn test"
	    sh "mvn verify"
	    sh "mvn validate"
	    sh "mvn clean package"
	    }
	  }
	}
}