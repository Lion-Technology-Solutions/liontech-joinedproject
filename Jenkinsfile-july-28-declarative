
pipeline{
      agent any  #will schedule job on any nodes
      tools{
            maven "maven3.9.8"
      }

      stages{
            stage('clonefromscm')
              steps{
                  sh "echo cloning latest changes from scm"
                  git branch: 'class26', url: 'https://github.com/Lion-Technology-Solutions/liontech-joinedproject.git'
              }
      }
}