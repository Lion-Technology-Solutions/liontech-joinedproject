pipeline{
  agent any 
  tools{
    maven "maven3.9.8"
  }
  stages{
    stage('clonefromscm'){
      steps{
        sh "echo  Jenkins is clonefromscm"
        git branch: 'class26', url: 'https://github.com/Lion-Technology-Solutions/liontech-joinedproject.git'

      }
    }

    stage('Maven-Build'){
      steps{
        sh "echo 'running Junit testing'"
        sh "echo'Maven is packing the source code'"
        sh "mvn validate"
        sh "mvn install"
        sh "mvn clean package"
      }
    }
  }
}